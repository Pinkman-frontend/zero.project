{
  "root": true,
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "jquery": true,
    "es6": true
  },
  "globals": {
    "tars": true
  },
  "extends": "airbnb-base",
  "rules": {
    "consistent-return": 0,
    "keyword-spacing": 2,
    "strict": 0,
    "no-confusing-arrow": 2,
    "prefer-arrow-callback": 0,
    "no-debugger": 0,
    "no-unused-vars": [1, {"vars": "all", "args": "after-used"}],
    "no-console": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": [
          "node_modules",
          "markup"
        ]
      }
    }
  }
}
